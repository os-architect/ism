<table>
{{#each-in tiles as |rown row| }}
    <tr>
        {{#each-in row as |celln cell| }}
            <Map-Tile
              @x={{celln}}
              @y={{rown}}
              @characters={{ cell.characters }}
              @objects={{ cell.objects }}
            />
        {{/each-in}}
    </tr>
{{/each-in}}
</table>